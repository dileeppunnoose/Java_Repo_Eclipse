
public class Hello {
	public static void main(String[] args){
        System.out.println("Hello World - Dileep");
        int myFirstNumber = (10+3) + (2*3);
        int mySecondNumber = 12;
        int myThirdNumber = mySecondNumber*10;
        int myTotal= myFirstNumber+mySecondNumber+myThirdNumber;
        int challengeQuestion= 1000-myTotal;
        System.out.println(myTotal);
        System.out.println(challengeQuestion);

        int a=10;
        byte b=11;
        short c=12;
        long d=50000*(a+b+c);
        System.out.println(d);
    }
}
